    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.2.0",
    "pg": "^8.11.3",
