{
  "name": "contentsharingplatform",
  "version": "1.0.0",
  "description": "A place for JASG players to share maps, mods, and more",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./server/index.js",
